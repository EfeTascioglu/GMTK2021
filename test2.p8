version 32
__lua__
function _init()
x = 64
y = 64
end

function _update()
	if (btn(⬅️)) x-=1
	if (btn(➡️)) x+=1
	if (btn(⬆️)) y-=1
	if (btn(⬇️)) y+=1	
end

function _draw()
	cls()
	spr(0,x,y)
end

